set nocompatible
set autoindent
set showmode
set showmatch
set showcmd
set title
set ruler
set number
set laststatus=2
set showtabline=2
set tabstop=4
set shiftwidth=4
set viminfo=
set list
set listchars=tab:->,trail:-
set nocindent
let lisp_raingbow = 1

" FILETYPE
au BufNewFile,BufRead *.clisp setf lisp
au BufNewFile,BufRead *.scheme setf lisp
au BufNewFile,BufRead *.h setf cpp
au BufNewFile,BufRead *m setf cpp
